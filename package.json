{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.ts",
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@react-pdf/renderer": "^2.1.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-router-dom": "^5.3.3",
    "assert": "^2.0.0",
    "bootstrap": "^5.1.3",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "concurrently": "^7.2.1",
    "electron-is": "^3.0.0",
    "electron-is-dev": "^2.0.0",
    "json-server": "^0.17.0",
    "orsys-tjs-meme": "^1.0.0",
    "process": "^0.11.10",
    "react": "^18.1.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "5",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.6.4",
    "util": "^0.12.4",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "json": "json-server -p 5679 db/db.json",
    "layoutcmp": "npx generate-react-cli c --type=layout",
    "electron:start": "concurrently \"yarn json\" \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "yarn build && electron-builder build --publish never",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.7",
    "@storybook/addon-essentials": "^6.5.7",
    "@storybook/addon-interactions": "^6.5.7",
    "@storybook/addon-links": "^6.5.7",
    "@storybook/builder-webpack5": "^6.5.7",
    "@storybook/manager-webpack5": "^6.5.7",
    "@storybook/node-logger": "^6.5.7",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.7",
    "@storybook/testing-library": "^0.0.11",
    "babel-plugin-named-exports-order": "^0.0.2",
    "electron": "^19.0.1",
    "electron-builder": "^23.0.3",
    "prop-types": "^15.8.1",
    "webpack": "^5.73.0"
  },
  "homepage": "./",
  "build": {
    "appId": "com.champix.react.tjs.fioulmarket",
    "productName": "React Electron App",
    "files": [
      "build/**/*",
      "electron/main.ts",
      "package.json"
    ],
    "extends": null,
    "mac": {
      "type": "distribution"
    }
  }
}
